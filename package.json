{
  "name": "vitetest",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "tw:watch": "chokidar './src/**/*.ts' -c 'npm run tw:build'",
    "tw:build": "tailwindcss -i ./src/lib/tailwind/tailwindlib.css -o ./src/lib/tailwind/tailwind.css && twlit --output ./src/lib/tailwind/index.ts --input ./src/lib/tailwind/tailwind.css",
    "lint": "eslint --ext .ts,.js ./src",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.5.0",
    "@storybook/addon-essentials": "^8.1.6",
    "@storybook/addon-links": "^8.1.6",
    "@storybook/blocks": "^8.1.6",
    "@storybook/test": "^8.1.6",
    "@storybook/web-components": "^8.1.6",
    "@storybook/web-components-vite": "^8.1.6",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "autoprefixer": "^10.4.19",
    "babel-eslint": "^10.1.0",
    "chokidar-cli": "^3.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-lit": "^1.14.0",
    "eslint-plugin-lit-a11y": "^4.1.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-wc": "^2.1.0",
    "pollen-css": "^4.6.2",
    "postcss": "^8.4.38",
    "prettier": "3.3.1",
    "storybook": "^8.1.6",
    "tailwindcss": "^3.4.4",
    "twlit": "^0.0.1-alpha.37",
    "typescript": "^5.4.5",
    "vite": "^5.2.13",
    "vite-plugin-html-inject": "^1.1.2",
    "vite-plugin-svgo": "^1.4.0",
    "vite-plugin-svgr": "^4.2.0"
  },
  "dependencies": {
    "boxicons": "^2.1.4",
    "jss": "^10.10.0",
    "jss-preset-default": "^10.10.0",
    "lit": "^3.1.4"
  }
}
