{
  "name": "vitetest",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tw:watch": "chokidar './src/**/*.ts' -c 'npm run tw:build'",
    "tw:build": "tailwindcss -i ./src/lib/tailwind/tailwindlib.css -o ./src/lib/tailwind/tailwind.css && twlit --output ./src/lib/tailwind/index.ts --input ./src/lib/tailwind/tailwind.css",
    "lint": "eslint --ext .ts,.js ./src"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "autoprefixer": "^10.4.16",
    "babel-eslint": "^10.1.0",
    "chokidar-cli": "^3.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-lit": "^1.11.0",
    "eslint-plugin-lit-a11y": "^4.1.1",
    "eslint-plugin-wc": "^2.0.4",
    "pollen-css": "^4.6.2",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "twlit": "^0.0.1-alpha.37",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-svgo": "^1.4.0",
    "vite-plugin-svgr": "^4.2.0"
  },
  "dependencies": {
    "@tabler/icons": "^2.46.0",
    "jss": "^10.10.0",
    "jss-preset-default": "^10.10.0",
    "lit": "^3.1.1"
  }
}
