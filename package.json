{
  "name": "vitetest",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite && npm run tw:watch",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tw:watch": "chokidar './src/**/*.ts' -c 'npm run tw:build'",
    "tw:build": "tailwindcss -i ./src/tailwind/tailwindlib.css -o ./src/tailwind/tailwind.css && twlit --output ./src/tailwind/index.ts --input ./src/tailwind/tailwind.css"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "twlit": "^0.0.1-alpha.37",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "dependencies": {
    "lit": "^3.1.1"
  },
  "_moduleAliases": {
		"@": "src"
	}
}
